{
  "name": "ecm-sampler",
  "version": "1.1.0",
  "description": "ecm sample sync app",
  "main": "src/app.ts",
  "private": true,
  "scripts": {
    "lint": "eslint '*.ts' --quiet --fix",
    "postinstall": "tsc",
    "start": "npm run build:ts && fastify start -l info dist/app.js -p 4000",
    "build:ts": "tsc",
    "test": "tsc -p test/tsconfig.test.json && cross-env TS_NODE_FILES=true tap -Rspec --timeout 120 --no-cov --ts test/**/*.test.ts",
    "sam-local-invoke:fetch": "sam local invoke 'EcmSamplerFetch' -e test/events/fetch-event.json",
    "sam-local-invoke:sync": "sam local invoke 'EcmSamplerSync' -e test/events/sync-event.json"
  },
  "author": "Mars Kim",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/aws-lambda": "^3.2.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/env": "^4.2.0",
    "@fastify/postgres": "^5.2.0",
    "axios": "^1.3.4",
    "bluebird": "^3.7.2",
    "chrome-aws-lambda": "^10.1.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.13.0",
    "fastify-cli": "^5.7.1",
    "pg": "^8.9.0",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.111",
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.38",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.1",
    "@types/pg": "^8.6.6",
    "@types/pg-format": "^1.0.2",
    "@types/tap": "^15.0.8",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fastify-tsconfig": "^1.0.1",
    "json-schema-to-ts": "^2.7.2",
    "prettier": "^2.8.4",
    "puppeteer": "^19.7.2",
    "tap": "^16.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
